{
  "name": "crows-run",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "docs": "hads ./docs -o",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run crows-run:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:server:prod": "ng run crows-run:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "ng build --prod",
    "build:prod": "npm run build:server:prod && npm run build:browser:prod",
    "server": "node local.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/forms": "^9.0.1",
    "@angular/http": "^6.1.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "@angular/platform-server": "^9.0.1",
    "@angular/router": "^9.0.1",
    "@ng-toolkit/universal": "^1.1.47",
    "@nguniversal/express-engine": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "angulartics2": "^6.0.0",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "domino": "^2.1.0",
    "error-stack-parser": "^2.0.2",
    "hads": "^1.7.0",
    "ngx-select-dropdown": "^1.3.1",
    "rxjs": "~6.5.4",
    "ts-loader": "4.2.0",
    "tslib": "^1.9.0",
    "ua-parser-js": "^0.7.21",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "zone.js": "~0.10.2"
  },
